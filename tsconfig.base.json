{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@notional-finance/about-us-feature-shell": [
        "packages/features/about-us/src/index.ts"
      ],
      "@notional-finance/about-us-feature-shell/*": [
        "packages/features/about-us/src/*"
      ],
      "@notional-finance/assets/*": ["packages/shared/assets/src/assets/*"],
      "@notional-finance/borrow-feature-shell": [
        "packages/features/borrow/src/index.ts"
      ],
      "@notional-finance/borrow-feature-shell/*": [
        "packages/features/borrow/src/lib/*"
      ],
      "@notional-finance/contracts": ["packages/contracts/src/index.ts"],
      "@notional-finance/contracts/*": ["packages/contracts/src/*"],
      "@notional-finance/core-entities": [
        "packages/core-entities/src/index.ts"
      ],
      "@notional-finance/error/*": ["packages/error/src/*"],
      "@notional-finance/helpers": ["packages/shared/helpers/src/index.ts"],
      "@notional-finance/icons": ["packages/common/icons/src/index.ts"],
      "@notional-finance/icons/*": ["packages/common/icons/src/*"],
      "@notional-finance/lend-feature-shell": [
        "packages/features/lend/src/index.ts"
      ],
      "@notional-finance/lend-feature-shell/*": [
        "packages/features/lend/src/*"
      ],
      "@notional-finance/liquidity-feature-shell": [
        "packages/features/liquidity/src/index.ts"
      ],
      "@notional-finance/liquidity-feature-shell/*": [
        "packages/features/liquidity/src/*"
      ],
      "@notional-finance/mui": ["packages/common/mui/src/index.ts"],
      "@notional-finance/mui/*": ["packages/common/mui/src/*"],
      "@notional-finance/multicall": ["packages/multicall/src/index.ts"],
      "@notional-finance/notionable": ["packages/notionable/src/index.ts"],
      "@notional-finance/notionable-hooks": [
        "packages/shared/notionable-hooks/src/index.ts"
      ],
      "@notional-finance/notionable-hooks/*": [
        "packages/shared/notionable-hooks/src/*"
      ],
      "@notional-finance/notionable/*": ["packages/notionable/src/*"],
      "@notional-finance/portfolio-feature-shell": [
        "packages/features/portfolio/src/index.ts"
      ],
      "@notional-finance/portfolio-feature-shell/*": [
        "packages/features/portfolio/src/*"
      ],
      "@notional-finance/risk-engine": ["packages/risk-engine/src/index.ts"],
      "@notional-finance/shared-web": ["packages/shared/web/src/index.ts"],
      "@notional-finance/shared-web/*": ["packages/shared/web/src/*"],
      "@notional-finance/side-drawer": [
        "packages/shared/side-drawer/src/index.ts"
      ],
      "@notional-finance/styles": ["packages/common/styles/src/index.ts"],
      "@notional-finance/styles/*": ["packages/common/styles/src/*"],
      "@notional-finance/trade": ["packages/shared/trade/src/index.ts"],
      "@notional-finance/trade/*": ["packages/shared/trade/src/*"],
      "@notional-finance/transaction": ["packages/transaction/src/index.ts"],
      "@notional-finance/util": ["packages/util/src/index.ts"],
      "@notional-finance/vault-feature-shell": [
        "packages/features/vault/src/index.ts"
      ],
      "@notional-finance/vault-feature-shell/*": [
        "packages/features/vault/src/*"
      ],
      "@notional-finance/wallet": ["packages/shared/wallet/src/index.ts"],
      "@notional-finance/wallet/*": ["packages/shared/wallet/src/*"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
