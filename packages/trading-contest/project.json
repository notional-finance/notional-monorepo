{
  "name": "trading-contest",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/trading-contest/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/trading-contest",
        "main": "packages/trading-contest/src/index.ts",
        "tsConfig": "packages/trading-contest/tsconfig.lib.json",
        "assets": ["packages/trading-contest/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/trading-contest/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/trading-contest/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
