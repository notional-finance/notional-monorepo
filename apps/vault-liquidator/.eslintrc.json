{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "plugins": ["address"],
  "rules": {
    "address/addr-type": ["error", "checksum"]
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
